<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
       "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">

<head>
  <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
  <title><%="Info"%></title>

  <%= stylesheet_link_tag 'style' %>
  <%= stylesheet_link_tag 'standard' %>
  <%= stylesheet_link_tag 'dhtmlxtabbar' %>
  <%= stylesheet_link_tag 'captcha' %>
  
  <%= javascript_include_tag :defaults %>
  <%= javascript_include_tag 'googlemaps' %>

  <%= javascript_include_tag 'prototype' %>
  <%= javascript_include_tag 'effects' %>
  <%= javascript_include_tag 'dragdrop' %>
  <%= javascript_include_tag 'controls' %>
  <%= javascript_include_tag 'scriptaculous' %>
  <%= javascript_include_tag 'dialog' %>
  <%= javascript_include_tag 'date-picker' %>
  <%= javascript_include_tag 'pwd_strength' %>
  
  <%= javascript_include_tag 'dhtmlxcommon' %>
  <%= javascript_include_tag 'dhtmlxtabbar' %>
  <%= javascript_include_tag 'dhtmlxtabbar_start' %>

</head>

<body>
  <table width="100%" cellspacing="0" cellpadding="0" border="0">
    <tr>
      <table width="100%" cellspacing="0" cellpadding="0" border="0">
      <td width="305" valign="top" height="180" class="header-tbl">
      	<img width="305" height="180" align="right" src= '/images/background/paper.jpg' />
      </td>
      <td width="401" valign="top" height="180" class="header-tbl">  
        <img width="401" height="180" align="left" src='/images/background/sity.jpg'/>
      </td>
      
      <td width="100%" valign="top" height="180" align="right" class="header-tbl">
    	<%# City.find(:all).each {|c| %>
    	  <%#= link_to c.name, {:controller => "/home" ,:action => 'index', :id => c.id} %>
    	<%#}%>
    	<div>
    	  <div style="margin: 0px;">
    	    <%= render :partial => "shared/logout" %> 
    	  </div>
    	  <%= render :partial => "shared/login" %>
        </div>   
	  </td>
	  </table>
    </tr>
 
    <tr>
      <table width="100%" cellspacing="0" cellpadding="0" border="0">
	      <td valign="top" height="220" class="under-header-tbl" colspan="2">
	        <img width="705" height="137" src='/images/background/logo.jpg' />
	      </td>
	      <td valign="top" height="220" class="under-header-tbl">
	      	<table border="0" cellspacing="10">
	          <tr>
	            <td align="center">
	            	<div class="menu"><a href="/home">
	            		<img src="/images/background/home-ico.jpg" alt="home" align="top" />
	                	<br />ГЛАВНАЯ</a>
	                </div>
	            </td>
	            <td align="center">
	            	<div class="menu"><a href="/search">
	            		<img src="/images/background/search-ico.jpg" alt="search" align="top" />
	                	<br />ПОИСК</a>
	                </div>
	            </td>
	            <td align="center">
	            	<div class="menu"><a href="/catalog">
	            		<img src="/images/background/catalog-ico.jpg" alt="catalog" align="top" />
	               		<br />КАТАЛОГ</a>
	               	</div>
	            </td>
	            <td align="center">
	            	<div class="menu"><a href="/brand">
	            		<img src="/images/background/brands-ico.jpg" alt="brands" align="top" />
	                	<br />БРЭНДЫ</a>
	                </div>
	            </td>
	          </tr>
	        </table>
	      </td>
      </table>	
    </tr>
        
    <tr>
    	 <table width="100%" cellspacing="0" cellpadding="0" border="0">
           <td width="200px" style="vertical-align: top;">
	        <table width="200px" border="0">
		      <tr>
		        <td>
		          <table border="0" cellpadding="0" cellspacing="0" width="100%">
		              <tr>
		                <td><img src="/images/background/corner-l-u.gif" /></td>
		                <td style="background: url(/images/background/corner-t.gif) repeat-x"><img src="/images/background/corner-t.gif" /></td>
		                <td><img src="/images/background/corner-r-u.gif" /></td>
		              </tr>
		              <tr>
		                <td style="background: url(/images/background/corner-l.gif) repeat-y"><img src="/images/background/corner-l.gif" /></td>
		                <td width="100%">
		                	<div class="user-menu">
		                    	<div style="text-align: right;">
		                            <img src="/images/background/user-menu.jpg" align="left" />
									<a href=""  onclick="  new Effect.toggle('menu', 'blind'); return false; "><img src="/images/background/min-but.gif" /></a>
									<a href="index.html"><img src="/images/background/full-but.gif" /></a>
		                        </div>
		                	   	<div id="menu">
				             		<%= render :partial => 'shared/menu' %>
				           		</div>
		                  </div>
		                </td>
		                <td style="background: url(/images/background/corner-r.gif) repeat-y"><img src="/images/background/corner-r.gif" /></td>
		              </tr>
		              <tr>
		                <td><img src="/images/background/corner-l-d.gif" /></td>
		                <td style="background: url(/images/background/corner-b.gif) repeat-x"><img src="/images/background/corner-b.gif" /></td>
		                <td><img src="/images/background/corner-r-d.gif" /></td>
		              </tr>
		           </table>
		         </td>
		       </tr>
		       <tr>
		        <td>
		        	<table border="0" cellpadding="0" cellspacing="0" width="100%">
		              <tr>
		                <td><img src="/images/background/corner-l-u.gif" /></td>
		                <td style="background: url(/images/background/corner-t.gif) repeat-x"><img src="/images/background/corner-t.gif" /></td>
		                <td><img src="/images/background/corner-r-u.gif" /></td>
		              </tr>
		              <tr>
		                <td style="background: url(/images/background/corner-l.gif) repeat-y"><img src="/images/background/corner-l.gif" /></td>
		                <td width="100%">
		                		<div style="text-align: right;">
		                            <img src="/images/background/news.gif" align="left" />
									<a href=""  onclick="  new Effect.toggle('news', 'blind'); return false; "><img src="/images/background/min-but.gif" /></a>
									<a href="index.html"><img src="/images/background/full-but.gif" /></a>
		                        </div>
		                        <div id="news">
		                        	<div class="blue-sep"><img src="/images/background/blue-sep.gif" /></div>
			                        <%= render(:partial => "shared/news") %>
		                        </div>
		                </td>
		                <td style="background: url(/images/background/corner-r.gif) repeat-y"><img src="/images/background/corner-r.gif" /></td>
		              </tr>
		              <tr>
		                <td><img src="/images/background/corner-l-d.gif" /></td>
		                <td style="background: url(/images/background/corner-b.gif) repeat-x"><img src="/images/background/corner-b.gif" /></td>
		                <td><img src="/images/background/corner-r-d.gif" /></td>
		              </tr>
		          </table>
		        </td>
		       </tr>
	        </table>
       </td>
      <td style="vertical-align: top;">	
      	<table border="0" cellpadding="0" cellspacing="0" class="content">
            	<tr>
                	<td colspan="2"><img src="/images/background/shad-up.gif" align="left" /></td>
                </tr>
                <tr>
                	<td style="vertical-align: top;">
                		<img src="/images/background/shad-left.gif" align="top" />
                	</td>
                    <td style="padding-left: 10px; padding-top: 5px;">
					        <div id="content">
					          <% unless flash[:error].blank? %>
					            <p style="color: red"><%= flash[:error] %></p>
					          <% end %>
					          <% unless flash[:notice].blank? %>
					            <p style="color: green"><%= flash[:notice] %></p>
					          <% end %>
					          <%= yield  %>
					        </div>
					</td>
				</tr>
			</table>
                
        <div style="clear:both;"></div>
       </div>
      </td>
    </table>
    </tr>
	    <tr>
	        <td align="center" style="height:35px;">
	          <div id="footer">
	            <div id="footer_left">
	              <%= link_to 'О проекте', {:controller => '/home', :action => 'about'}, :target => '_blank' %>
	              |
	              <%= link_to 'Контакты', {:controller => '/home', :action => 'contact'}, :target => '_blank' %>
	            </div>
	        </td>
	        <td>    
	            <div id="footer_right">
	            Copyright  © 2008 
	            </div>
	          </div>
	        </td>
	     </tr>
      </table>
    </td>
   </tr>
  </table>
  <div id="overlay_div"></div>
  <div id="internal_dialog"></div>
</body>

</html>
